#include <stdio.h>
#include <stdlib.h>

int main()
{
  float a,b,c,d,e,sum,per;
  	printf("Enter Marks of the student: ");
	scanf("%f %f %f %f %f",&a,&b,&c,&d,&e);
 	sum=a+b+c+d+e;
	per=sum/5;
	printf("\nSum %f",sum);
    	printf("\nPercentage: %f",per);
    
    if(per<60)
       	printf("\nGrade: C");
    else if(per>60&&per<70)
       	printf("\nGrade: B");
       	
    else if(per<70&&per<80)
       	printf("\nGrade: A");
       	
    else if(per>80&&per<90)
       	printf("\nGrade: S");
    
    else
       	printf("\n\n\tGod MODE");

	return 0;
}
