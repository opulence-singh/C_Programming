#include <stdio.h>
#include <stdlib.h>

int main()
{
   int a,b,c;
	printf("Enter 3 numbers: ");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b && a>c)
	{
		printf("%d",a);
		printf(" is greatest");
	}
	else if(b>c)
	{
		printf("%d",b);
		printf(" is greatest");
	}
	else
	{
		printf("%d",c);
		printf(" is greatest");	
	}
	return 0;
}
